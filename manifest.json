{
  "name": "Return to Twitter",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "assets/16.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "description": "X out the X and return to Twitter lmao",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://x.com/*"
      ],
      "css": [
        "inject.css"
      ],
      "js": [
        "favicon.js"
      ]
    }
  ]
}